<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.booxtown.fragment.MyProfileDashboardFragment">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/menu_myprofile"
        android:id="@+id/include50" />

    <com.booxtown.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Dashboard"
        android:id="@+id/textView89"
        android:layout_below="@+id/include50"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/color_text"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp" />

    <com.booxtown.custom.CustomTextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:text="New Text"
        android:id="@+id/textView98"
        android:layout_below="@+id/textView89"
        android:background="@color/color_text_hint"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView98"
        android:layout_alignParentTop="false"
        android:gravity="top"
        android:layout_alignParentLeft="false"
        android:layout_alignParentBottom="false"
        android:orientation="horizontal"
        android:background="@color/dot_light_screen1"
        android:id="@+id/linearLayout2">

        <RelativeLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.65">

            <com.booxtown.custom.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Book"
                android:id="@+id/textView90"
                android:textColor="@color/color_title_book"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp" />

            <com.booxtown.custom.CustomTextView
                android:layout_width="1dp"
                android:layout_height="30dp"
                android:id="@+id/textView94"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:background="@color/color_text_hint"/>

        </RelativeLayout>

        <RelativeLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <com.booxtown.custom.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Offering"
                android:id="@+id/textView91"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:textColor="@color/color_title_book"/>

            <com.booxtown.custom.CustomTextView
                android:layout_width="1dp"
                android:layout_height="30dp"
                android:id="@+id/textView95"
                android:gravity="right"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:background="@color/color_text_hint"/>
        </RelativeLayout>

        <RelativeLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <com.booxtown.custom.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Status"
                android:id="@+id/textView92"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:textColor="@color/color_title_book"/>

            <com.booxtown.custom.CustomTextView
                android:layout_width="1dp"
                android:layout_height="30dp"
                android:id="@+id/textView96"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:background="@color/color_text_hint"/>
        </RelativeLayout>

        <RelativeLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.9">

            <com.booxtown.custom.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rating"
                android:id="@+id/textView93"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:textColor="@color/color_title_book"/>

        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout2"
        >
    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lv_myprofile_dashboard"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="false" />
    </LinearLayout>
</RelativeLayout>
</FrameLayout>
