<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.booxtown.fragment.AboutFragment"
    android:background="@color/dot_light_screen1"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    <com.booxtown.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/about_booxtown"
        android:id="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textColor="@color/color_topic_interact"
        android:padding="15dp" />
    </ScrollView>
</FrameLayout>
